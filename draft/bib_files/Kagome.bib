  title = {Filling-Enforced Obstructed Atomic Insulators},
  author = {Xu, Yuanfeng and Elcoro, Luis and Song, Zhi-Da and Vergniory, M. G. and Felser, Claudia and Parkin, Stuart S. P. and Regnault, Nicolas and Ma{\~n}es, Juan L. and Bernevig, B. Andrei},
